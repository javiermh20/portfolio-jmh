---
import AndroidIcon from "../icons/AndroidIcon.astro"
import AppleIcon from "../icons/AppleIcon.astro"
import AstroIcon from "../icons/AstroIcon.astro"
import DartIcon from "../icons/DartIcon.astro"
import FirebaseIcon from "../icons/FirebaseIcon.astro"
import FlutterIcon from "../icons/FlutterIcon.astro"
import GitIcon from "../icons/GitIcon.astro"
import ReactIcon from "../icons/ReactIcon.astro"
import SwiftIcon from "../icons/SwiftIcon.astro"
import PostmanIcon from "../icons/PostmanIcon.astro"
import TypeScriptIcon from "../icons/TypeScriptIcon.astro"
import MetaIcon from "../icons/MetaIcon.astro"
import VisualStudioIcon from "../icons/VisualStudioIcon.astro"
---
<div class="container mx-auto">
<div class="w-full inline-flex flex-nowrap">
    <ul class="flex items-center justify-start [&_li]:mx-8 [&_img]:max-w-none animate-infinite-scroll">
        <li>
            <FlutterIcon class="size-10" />
        </li>
        <li>
            <ReactIcon class="size-10" />
        </li>
        <li>
            <AppleIcon class="size-10"/>
        </li>
        <li>
            <AndroidIcon class="size-12"/>
        </li>
        <li>
            <DartIcon class="size-10"/>
        </li>
        <li>
            <SwiftIcon class="size-10"/>
        </li>
        <li>
            <VisualStudioIcon class="size-10"/>
        </li>
        <li>
            <FirebaseIcon class="size-10"/>
        </li>
        <li>
            <GitIcon class="size-10"/>
        </li>
        <li>
            <AstroIcon class="size-10 mt-2"/>
        </li>
        <li>
            <TypeScriptIcon class="size-10" />
        </li>
        <li>
            <PostmanIcon class="size-10"/>
        </li>
        <li>
            <MetaIcon class="size-10"/>
        </li>
    </ul>
    <ul class="flex items-center justify-center md:justify-start [&_li]:mx-8 [&_img]:max-w-none animate-infinite-scroll" aria-hidden="true">
        <li>
            <FlutterIcon class="size-10" />
        </li>
        <li>
            <ReactIcon class="size-10" />
        </li>
        <li>
            <AppleIcon class="size-10"/>
        </li>
        <li>
            <AndroidIcon class="size-12"/>
        </li>
        <li>
            <DartIcon class="size-10"/>
        </li>
        <li>
            <SwiftIcon class="size-10"/>
        </li>
        <li>
            <VisualStudioIcon class="size-10"/>
        </li>
        <li>
            <FirebaseIcon class="size-10"/>
        </li>
        <li>
            <GitIcon class="size-10"/>
        </li>
        <li>
            <AstroIcon class="size-10 mt-2"/>
        </li>
        <li>
            <TypeScriptIcon class="size-10" />
        </li>
        <li>
            <PostmanIcon class="size-10"/>
        </li>
        <li>
            <MetaIcon class="size-10"/>
        </li>
    </ul>
</div>
</div>